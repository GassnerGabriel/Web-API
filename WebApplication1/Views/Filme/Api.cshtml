@section Scripts{
    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/filme")
}

<table>
    <thead>
        <tr>
            <th>Código </th>
            <th>Titulo </th>
            <th>Ano</th>
            <th>Genero</th>
            <th>Produtora</th>
            <th>Operações</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: filmes">
        <tr>
            <td data-bind="text: Codigo"></td>
            <td data-bind="text: Titulo"></td>
            <td data-bind="text: Ano"></td>
            <td data-bind="text: Genero"></td>
            <td data-bind="text: Produtora"></td>
            <td>
                <button>Editar</button>
                <button>Excluir</button>
            </td>
        </tr>
    </tbody>
</table>